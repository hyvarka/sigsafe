SConscript('race_checker/SConscript')

Import('env')
env = env.Copy()

for i in ['blocked_alarm',
          'setitimer_rounding',
          'socket_timeout',
          'sigsuspend']:
    env.Program(target = 'test_' + i, source = 'test_' + i + '.c')
